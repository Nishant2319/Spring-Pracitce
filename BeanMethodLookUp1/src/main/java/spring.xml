<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   
   
	<bean id="stu" class="com.BeanMethodLookUp1.Student">
		<lookup-method name="look" bean="Bo"/>
		<constructor-arg index="0" value="1"></constructor-arg>
		<constructor-arg type="String" value="Nishant"></constructor-arg>
	</bean>
	
	<bean name="Bo" class="com.BeanMethodLookUp1.Boy" scope="prototype">
		<constructor-arg type="int" value="19"></constructor-arg>
	</bean>
	
</beans>