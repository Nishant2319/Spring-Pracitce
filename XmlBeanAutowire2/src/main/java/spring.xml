<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">



	<bean id="stu" class="com.XmlBeanAutowire2.Student"
		autowire="constructor">
		<property name="name" value="Nishant"></property>
		<property name="roll" value="1"></property>
	</bean>

	<bean name="boy" class="com.XmlBeanAutowire2.Boy">
		<constructor-arg type="int" value="19"></constructor-arg>
	</bean>

</beans>